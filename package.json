{
  "name": "@adobe/helix-fetch",
  "version": "2.0.0",
  "description": "Lightweight Fetch implementation transparently supporting both HTTP/1(.1) and HTTP/2",
  "main": "src/index.js",
  "scripts": {
    "test": " nyc --reporter=text --reporter=lcov --check-coverage --branches 90 --statements 90 --lines 90 mocha --recursive",
    "test-ci": "nyc --reporter=text --reporter=lcov --check-coverage --branches 90 --statements 90 --lines 90 mocha --recursive --reporter xunit --reporter-options output=./junit/test-results.xml && codecov",
    "lint": "./node_modules/.bin/eslint .",
    "semantic-release": "semantic-release",
    "docs": "npx jsdoc2md -c .jsdoc.json --files 'src/*.js'  > docs/API.md",
    "commit": "git-cz"
  },
  "engines": {
    "node": ">=12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adobe/helix-fetch"
  },
  "author": "",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adobe/helix-fetch/issues"
  },
  "homepage": "https://github.com/adobe/helix-fetch#readme",
  "dependencies": {
    "debug": "4.3.1",
    "form-data": "3.0.0",
    "get-stream": "6.0.0",
    "http-cache-semantics": "4.1.0",
    "lru-cache": "6.0.0",
    "object-sizeof": "1.6.1"
  },
  "devDependencies": {
    "@adobe/eslint-config-helix": "1.1.3",
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/commit-analyzer": "8.0.1",
    "@semantic-release/git": "9.0.0",
    "@semantic-release/github": "7.2.0",
    "@semantic-release/npm": "7.0.9",
    "@semantic-release/release-notes-generator": "9.0.1",
    "@types/node": "^14.14.2",
    "cd": "^0.3.3",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-iterator": "^3.0.2",
    "codecov": "3.8.1",
    "commitizen": "4.2.2",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.16.0",
    "eslint-plugin-header": "3.1.0",
    "eslint-plugin-import": "2.22.1",
    "is-stream": "2.0.0",
    "istanbul": "1.1.0-alpha.1",
    "jsdoc-to-markdown": "6.0.1",
    "junit-report-builder": "2.1.0",
    "lint-staged": "10.5.3",
    "mocha": "8.2.1",
    "nock": "13.0.5",
    "nyc": "15.1.0",
    "parse-cache-control": "1.0.1",
    "pem": "^1.14.4",
    "semantic-release": "17.3.0",
    "sinon": "^9.2.1",
    "stream-buffers": "3.0.2"
  },
  "lint-staged": {
    "*.js": "eslint"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npx lint-staged"
    }
  }
}
